---
permalink: /types/
layout: layouts/home.njk
title: Types
templateClass: _container
hello: false
---

<h1 class="_text-align-center">
  {{ title }}
</h1>

<div class="_text-align-center">
  {%- for type in collections.typeList -%}
    {% set typeUrl %}/type/{{ type | slug | plural }}/{% endset %}

    <a href="{{ typeUrl | url }}" class="tag -large">
      {{ type }}
    </a>
  {%- endfor -%}
</div>