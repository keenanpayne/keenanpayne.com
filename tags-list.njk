---
permalink: /tags/
layout: layouts/home.njk
templateClass: page-tags
---

<h1 class="_text-align-center">
  Tags
</h1>

<div class="_text-align-center">
{%- for tag in collections.tagList -%}
  {% set tagUrl %}/tags/{{ tag | slug }}/{% endset %}

  <a href="{{ tagUrl | url }}" class="post-tag -large">
    {{ tag }}
  </a>
{%- endfor -%}
</div>